{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@/assets/*": ["assets/*"],
      "@/data/*" : ["data/*"],
      "@/components/*": ["components/*"],
      "@/pages/*": ["pages/*"],
      "@/utils/*": ["utils/*"],
      "@/providers/*": ["providers/*"],
      "@/styles/*": ["styles/*"],
    }
  },
  "exclude": ["node_modules"],
}